
#include <stdio.h>

int main()
{
    FILE *ark;
    char nome[10], *resultado;
    
        ark = fopen("arquivo.txt", "r");
        
        if(ark){
            while (!feof(ark)){//File End of File
            resultado = fgets(nome, 10, ark);
            printf("Resultado: %d\n", resultado);
            if(resultado){
                printf("%s", nome);
            }
        }
        }else{
            printf("NÃ£o existe");
        }
        
        fclose(ark);
        
    return 0;
}
