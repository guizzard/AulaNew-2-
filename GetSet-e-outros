public class ContaBancaria {
    private double saldo;
    private String titular;
    private String numeroDaConta;

    public ContaBancaria(String titular, String numeroDaConta) {
        this.titular = titular;
        this.numeroDaConta = numeroDaConta;
        this.saldo = 0;
    }

    public void depositar(double valor) {
        if (valor > 0) {
            saldo += valor;
        }
    }

    public boolean sacar(double valor) {
        if (valor > 0 && valor <= saldo) {
            saldo -= valor;
            return true;
        }
        return false;
    }

    public double consultarSaldo() {
        return saldo;
    }

    public String getTitular() {
        return titular;
    }

    public void setTitular(String titular) {
        this.titular = titular;
    }

    public String getNumeroDaConta() {
        return numeroDaConta;
    }
}
//outra classe
public class Main {
    public static void main(String[] args) {
        ContaBancaria conta = new ContaBancaria("Jo찾o Silva", "12345-6");

        conta.depositar(1000);
        System.out.println("Saldo ap처s dep처sito: " + conta.consultarSaldo());

        boolean saque1 = conta.sacar(500);
        System.out.println("Saque de 500: " + (saque1 ? "Sucesso" : "Falha"));
        System.out.println("Saldo ap처s saque: " + conta.consultarSaldo());

        boolean saque2 = conta.sacar(600);
        System.out.println("Saque de 600: " + (saque2 ? "Sucesso" : "Falha"));
        System.out.println("Saldo final: " + conta.consultarSaldo());
    }
}
